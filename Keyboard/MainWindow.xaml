                <TextBlock FontSize="15" Text="Speed: "/>
                <TextBlock Name="Speed" FontSize="15" FontWeight="Bold" Text="0"/>
                <TextBlock FontSize="15" Text=" chars/min"/>
            </StackPanel>
            <StackPanel Grid.Column="1" Orientation="Horizontal">
                <TextBlock FontSize="15" Text="Fails: "/>
                <TextBlock FontSize="15" FontWeight="Bold" Name="Fails" Text="0"/>
            </StackPanel>
            <Grid Grid.Column="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Column="0" Orientation="Horizontal">
                    <TextBlock Name ="Difficulty" FontSize="15" Text="Difficulty: "/>
                    <TextBlock Name ="DifficultyNomber" FontSize="15" FontWeight="Bold" Text="0"/>
                </StackPanel>
                <Slider Grid.Column="1" Name ="Slider" HorizontalAlignment="Stretch" Margin="10,0,10,0" VerticalAlignment="Center" Maximum="5" Minimum="1" ValueChanged="Slider_ValueChanged" Value="1" LargeChange="1" SmallChange="1" Cursor="Arrow" TickPlacement="Both" />
            </Grid>
            <Grid Grid.Column="3">
                <CheckBox FontSize="15" VerticalContentAlignment="Center">Case Sensitive</CheckBox>
            </Grid>
        </Grid>
        <Grid Grid.Row="1" Margin="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="1">
